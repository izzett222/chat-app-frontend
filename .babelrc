  {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              ">0.25%",
              "not ie 11",
              "not op_mini all"
            ],
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/syntax-dynamic-import",
      "@babel/plugin-transform-runtime"
    ],
    "env": {
      "test": {
        "plugins": ["dynamic-import-node"]
      }
    }
  }
